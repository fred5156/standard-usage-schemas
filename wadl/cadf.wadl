                                <entry>
                                    <para>
                                        <code>queryString</code>
                                    </para>
                                </entry>
                                <entry>
                                    <para>
                                        An optional field. Query string is the part of
                                        a uri containing data which is added to a base uri.
                                    </para>
                                </entry>
                                <entry>
                                    <para>string</para>
                                </entry>
