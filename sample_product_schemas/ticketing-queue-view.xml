<productSchema xmlns="http://docs.rackspace.com/core/usage/schema"
               namespace="http://docs.rackspace.com/event/ticketing/queue"
               serviceCode="Ticketing"
               version="1" 
               resourceTypes="VIEW"
               type="UPDATE"
               writeRole="cloudfeeds:ticketing-publisher">
    <description>
        Queue View definition update event.
    </description>
    <attribute name="id" type="string" use="required">
        The unique identifier for the queue view
    </attribute>
    <attribute name="number" type="int" use="required">
        Queue Id/number at Source
    </attribute>
    <attribute name="name" type="string" use="required">
        Queue view name
    </attribute>
    <attribute name="sourceSystem" type="string" use="required" allowedValues="CORE ENCORE">
        Source System
    </attribute>
    <attribute name="sourceQuery" type="string" use="required">
        Source query
    </attribute>
    <attribute name="queueViewUpdateTime" type="dateTime" use="required">
        The unique identifier for the ticket
    </attribute>
</productSchema>